<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="struts-batchParam"  extends="default" namespace="/batchParam" >
		<action name="*_*" method="{1}" class="{2}">
			<result name="showBatchParamIndexPage">/batch/pages/batchParam/batchParamIndex.jsp</result>
			<result name="showInsertBatchParamPage">/batch/pages/batchParam/batchParamAdd.jsp</result>
			<result name="showUpdateBatchParamPage">/batch/pages/batchParam/batchParamUpdate.jsp</result>
		</action>
	</package>
</struts>