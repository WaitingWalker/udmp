<struts>
	<package name="struts-batchMonitor"  extends="default" namespace="/batchMonitor" >
	<action name="*_*" method="{1}" class="{2}">
		<result name="showBatchMonitorIndexPage">/batch/pages/batchMonitor/batchMonitorIndex.jsp</result>
		<result name="showBatchMonitorDetailPage">/batch/pages/batchMonitor/batchMonitorDetail.jsp</result>
